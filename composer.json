{
    "name": "triverla/laravel-monnify",
    "description": "A laravel package for Monnify",
    "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "payments",
        "transactions",
        "subscription",
        "monnify",
        "monnify.com",
        "laravel 6",
        "laravel 7",
        "laravel 8",
        "laravel 9",
        "laravel 10"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Triverla\\LaravelMonnify\\": "src"
        }
    },
    "authors": [
        {
            "name": "Benaiah Yusuf",
            "email": "yusufbenaiah@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/support":  "~6|~7|~8|~9|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "~6|~7|~8|~9",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^12.0",
        "mockery/mockery": "1.*",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Triverla\\LaravelMonnify\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Triverla\\LaravelMonnify\\MonnifyServiceProvider"
            ],
            "aliases": {
                "Monnify": "Triverla\\LaravelMonnify\\Facades\\Monnify"
            }
        }
    }
}
